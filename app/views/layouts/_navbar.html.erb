<body>

<nav>
  <ul>
    <li><strong>Marketplace</strong></li>
  </ul>
  <ul>
    <% if session[:user_id].nil? %>
      <li><a><%= link_to "Register", register_path %></a></li>
      <li><a><%= link_to "Login", login_path %></a></li>
    <% else %>
      <li><a><%= link_to "Vendre un produit", product_new_path %></a></li>
      <li><a><%= link_to "Logout", logout_path, data: { turbo_method: :delete } %></a></li>

    <% end %>
  </ul>
</nav>

</body>